    <script src="{{ asset('bundles/metronic8/plugins/jquery-minicolors/jquery.minicolors.min.js') }}"></script>
    <script src="{{ asset('bundles/metronic8/plugins/bootstrap-fileinput/bootstrap-fileinput.js') }}"></script>

    <script src="{{ asset('bundles/metronic8/js/pages/data-tracking.js') }}?{{ 'now' | date('U') }}"></script>
    <script src="{{ asset('bundles/metronic8/js/pages/data-tracking-detalle.js') }}?{{ 'now' | date('U') }}"></script>
    <script>

        var permiso = {
            agregar: {% if permiso.agregar %}{{ permiso.agregar }}{% else %}0{% endif %}, // 1 o 0
            editar: {% if permiso.editar %}{{ permiso.editar }}{% else %}0{% endif %},
            eliminar: {% if permiso.eliminar %}{{ permiso.eliminar }}{% else %}0{% endif %}
        };

        var all_items = {{ items|json_encode(constant('JSON_PRETTY_PRINT'))|raw }};

        var direccion_url = "{{ direccion_url }}";

        jQuery(document).ready(function () {

            <!-- init components -->
            ModalInspector.init();
            ModalItemProject.init();
            ModalEmployee.init();
            ModalEmployeeSubcontractor.init();
            ModalConcreteVendor.init();

            DataTracking.init();

            DataTrackingDetalle.init();
        });
    </script>
{% endblock %}
