                                            <option value="{{ item.subcontractorId }}">
                                                {{ item.name }}
                                            </option>
                                        {% endfor %}
                                    </select>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12" id="select-employee-subcontractor">
                                    <label class="form-label required">
                                        Employee
                                    </label>
                                    <div class="input-group input-group-solid flex-nowrap">
                                        <div class="overflow-hidden flex-grow-1">
                                            <select id="employee-subcontractor"
                                                    class="form-control form-select form-select2 select-modal-labor select-validate form-select-solid rounded-end-0 border-end">
                                                <option value="">Select</option>
                                            </select>
                                        </div>
                                        <span class="input-group-text" title="Add employee"
                                              id="btn-add-employee-subcontractor"
                                              style="cursor: pointer;">
                                          <i class="ki-duotone ki-plus fs-3"><span class="path1"></span><span
                                                      class="path2"></span><span class="path3"></span><span
                                                      class="path4"></span></i>
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="form-label required">
                                Hours
                            </label>
                            <div class="row">
                                <div class="col-md-6">
                                    <input type="text" class="form-control form-control-validate form-control-solid form-control-number"
                                           id="hours" name="hours">
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="form-label">
                                Position/Role
                            </label>
                            <input type="text" class="form-control form-control-validate form-control-solid" id="labor-role" name="role"/>
                        </div>

                        <div class="form-group">
                            <label class="form-label">
                                Color
                            </label>
                            <div class="row">
                                <div class="col-md-6">
                                    <input type="text" class="form-control form-control-validate form-control-solid" id="labor-color" name="color"/>
                                </div>
                            </div>

                        </div>

                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-success" id="btn-salvar-data-tracking-labor">Save</button>
                    <button type="button" class="btn btn-danger" data-bs-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>

    <!-- material -->
    <div class="modal fade" id="modal-data-tracking-material" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">
                        New Material
                    </h5>

                    <div class="btn btn-icon btn-sm btn-active-light-primary ms-2" data-bs-dismiss="modal"
                         aria-label="Close">
                        <i class="ki-duotone ki-cross fs-1"><span class="path1"></span><span class="path2"></span></i>
                    </div>

                </div>
                <div class="modal-body">
                    <form class="m-form" id="data-tracking-material-form" action="#"
                          enctype="multipart/form-data" method="post">

                        <input type="hidden" id="data_tracking_material_id" name="data_tracking_material_id"/>

                        <div class="form-group">
                            <label class="form-label required">
                                Material
                            </label>

                            <div class="row">
                                <div class="col-md-12" id="select-material">
                                    <select class="form-control form-select form-select2 select-validate form-select-solid select-modal-material"
                                            id="material">
                                        <option value="">Select</option>
                                        {% for item in materials %}
                                            <option value="{{ item.materialId }}"
                                                    data-unit="{{ item.unit.description }}"
                                                    data-price="{{ item.price }}">
                                                {{ item.name }}
                                            </option>
                                        {% endfor %}
                                    </select>
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="row">
                                <div class="col-md-6">
                                    <label class="form-label required">
                                        Unit
                                    </label>
                                    <input type="text"
                                           class="form-control form-control-validate form-control-solid"
                                           id="material-unit"
                                           name="unit" disabled>
                                </div>
                                <div class="col-md-6">
                                    <label class="form-label required">
                                        Price
                                    </label>
                                    <div class="input-group input-group-solid">
                                        <span class="input-group-text">$</span>
                                        <input type="text"
                                               class="form-control form-control-validate form-control-price event-change"
                                               id="material-price" name="price" disabled/>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="form-label required">
                                Quantity
                            </label>
                            <div class="row">
                                <div class="col-md-6">
                                    <input type="text"
                                           class="form-control form-control-validate form-control-solid form-control-number"
                                           id="material-quantity" name="quantity">
                                </div>
                            </div>
                        </div>

                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-success" id="btn-salvar-data-tracking-material">Save</button>
                    <button type="button" class="btn btn-danger" data-bs-dismiss="modal">Close</button>

                </div>
            </div>
        </div>
    </div>

    <!-- conc vendor -->
