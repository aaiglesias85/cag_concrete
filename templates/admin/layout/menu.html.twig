                                                        <!--begin:Menu link-->
                                                        <a class="menu-link {{ activeRoute == 'project_stage' ? 'active' }}"
                                                           href="{{ url('project_stage') }}">
                                                        <span class="menu-bullet">
                                                            <span class="bullet bullet-dot"></span>
                                                        </span>
                                                            <span class="menu-title">Project Stages</span>
                                                        </a>
