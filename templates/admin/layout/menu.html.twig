                            <!--begin:Menu sub-->
                            <div class="menu-sub menu-sub-accordion">
                                <!--begin:Menu item-->
                                {% if menu.menuCompany %}
                                    <div class="menu-item">
                                        <a class="menu-link {{ activeRoute == 'company' ? 'active' }}"
                                           href="{{ url('company') }}">
                                        <span class="menu-bullet">
                                            <span class="bullet bullet-dot"></span>
                                        </span>
                                            <span class="menu-title">Companies</span>
                                        </a>
                                    </div>
                                {% endif %}
                                <!--end:Menu item-->
                                <!--begin:Menu item-->
                                {% if menu.menuProject %}
