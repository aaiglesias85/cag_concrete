                                                           href="{{ url('plan_downloading') }}">
                                                        <span class="menu-bullet">
                                                            <span class="bullet bullet-dot"></span>
                                                        </span>
                                                            <span class="menu-title">Plans Downloading</span>
                                                        </a>
                                                    </div>
                                                {% endif %}
