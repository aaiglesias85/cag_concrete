                                    <div class="menu-item">
                                        <a class="menu-link {{ activeRoute == 'conc_vendor' ? 'active' }}"
                                           href="{{ url('conc_vendor') }}">
                                        <span class="menu-bullet">
                                            <span class="bullet bullet-dot"></span>
                                        </span>
                                            <span class="menu-title">Concrete Vendors</span>
                                        </a>
                                    </div>
                                {% endif %}
                                <!--end:Menu item-->
                            </div>
