                                <a class="menu-link {{ activeRoute == 'data_tracking' ? 'active' }}"
                                   href="{{ url('data_tracking') }}">
                                <span class="menu-icon">
                                    <i class="ki-duotone ki-design-frame fs-2">
                                     <span class="path1"></span>
                                     <span class="path2"></span>
                                    </i>
                                </span>
                                    <span class="menu-title">Data Tracking</span>
                                </a>
                            </div>
                        {% endif %}

                    {% endif %}

                    <!-- menu invoice -->
                    {% if menu.menuInvoice %}
                        <div class="menu-item pt-5">
                            <div class="menu-content">
                                <span class="menu-heading fw-bold text-uppercase fs-7">Accounting</span>
                            </div>
                        </div>

                        <div class="menu-item">
                            <a class="menu-link {{ activeRoute == 'invoice' ? 'active' }}" href="{{ url('invoice') }}">
                                <span class="menu-icon">
                                    <i class="ki-duotone ki-bank fs-2">
                                        <span class="path1"></span>
                                        <span class="path2"></span>
                                    </i>
                                </span>
                                <span class="menu-title">Invoices and Payments</span>
                            </a>
                        </div>

                    {% endif %}

                    <!-- menu reportes -->
                    {% if menu.menuReporteSubcontractor or menu.menuReporteEmployee %}

                        <!--begin:Menu item-->
                        <div data-kt-menu-trigger="click" class="menu-item menu-accordion
                            {{ activeRoute == 'reporte_subcontractor' or activeRoute == 'reporte_employee' ? 'here show' }}
                        ">
                            <!--begin:Menu link-->
                            <span class="menu-link">
                                <span class="menu-icon">
                                    <i class="ki-duotone ki-book-open fs-2">
                                         <span class="path1"></span>
                                         <span class="path2"></span>
                                         <span class="path3"></span>
                                         <span class="path4"></span>
                                    </i>
                                </span>
