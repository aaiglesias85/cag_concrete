                                                </span>
                                                <span class="menu-title">Equations</span>
                                            </a>
                                        </div>
                                    {% endif %}
                                    <!--end:Menu item-->
                                    <!--begin:Menu item-->
                                    {% if menu.menuItem %}
                                        <div class="menu-item">
                                            <a class="menu-link {{ activeRoute == 'item' ? 'active' }}"
                                               href="{{ url('item') }}">
                                                <span class="menu-bullet">
                                                    <span class="bullet bullet-dot"></span>
                                                </span>
                                                <span class="menu-title">Items</span>
                                            </a>
                                        </div>
                                    {% endif %}
                                    <!--end:Menu item-->
                                    <!--begin:Menu item-->
                                    {% if menu.menuInspector %}
                                        <div class="menu-item">
                                            <a class="menu-link {{ activeRoute == 'inspectors' ? 'active' }}"
                                               href="{{ url('inspectors') }}">
                                                <span class="menu-bullet">
                                                    <span class="bullet bullet-dot"></span>
                                                </span>
                                                <span class="menu-title">Inspectors</span>
                                            </a>
                                        </div>
