                                <span class="menu-icon">
                                    <i class="ki-duotone ki-abstract-25 fs-2">
                                        <span class="path1"></span>
                                        <span class="path2"></span>
                                    </i>
                                </span>
                                <span class="menu-title">Estimates</span>
                                <span class="menu-arrow"></span>
                            </span>
                                <!--end:Menu link-->
                                <!--begin:Menu sub-->
                                <div class="menu-sub menu-sub-accordion">
                                    <!--begin:Menu item-->
                                    {% if menu.menuEstimate %}
                                        <div class="menu-item">
                                            <a class="menu-link {{ activeRoute == 'estimate' ? 'active' }}"
                                               href="{{ url('estimate') }}">
                                                <span class="menu-bullet">
                                                    <span class="bullet bullet-dot"></span>
                                                </span>
                                                <span class="menu-title">Project Estimates</span>
                                            </a>
                                        </div>
                                    {% endif %}
                                    <!--end:Menu item-->

                                    {% if menu.menuProjectStage or menu.menuProjectType or menu.menuProposalType or menu.menuPlanStatus or menu.menuDistrict or menu.menuPlanDownloading %}

                                        <!--begin:Menu item-->
                                        <div data-kt-menu-trigger="click" class="menu-item menu-accordion
