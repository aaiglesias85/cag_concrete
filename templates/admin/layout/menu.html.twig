                                <span class="menu-arrow"></span>
                            </span>
                            <!--end:Menu link-->
                            <!--begin:Menu sub-->
                            <div class="menu-sub menu-sub-accordion">
                                <!--begin:Menu item-->
                                {% if menu.menuReporteSubcontractor %}
                                    <div class="menu-item">
                                        <a class="menu-link {{ activeRoute == 'reporte_subcontractor' ? 'active' }}"
                                           href="{{ url('reporte_subcontractor') }}">
                                        <span class="menu-bullet">
                                            <span class="bullet bullet-dot"></span>
                                        </span>
                                            <span class="menu-title">Subcontractors</span>
                                        </a>
                                    </div>
                                {% endif %}
                                <!--end:Menu item-->
                                <!--begin:Menu item-->
                                {% if menu.menuReporteEmployee %}
                                    <div class="menu-item">
                                        <a class="menu-link {{ activeRoute == 'reporte_employee' ? 'active' }}"
                                           href="{{ url('reporte_employee') }}">
                                        <span class="menu-bullet">
                                            <span class="bullet bullet-dot"></span>
                                        </span>
                                            <span class="menu-title">Employees</span>
                                        </a>
                                    </div>
                                {% endif %}
                                <!--end:Menu item-->
                            </div>
                            <!--end:Menu sub-->
                        </div>
                        <!--end:Menu item-->

                    {% endif %}

