                                                    <div class="menu-item">
                                                        <a class="menu-link {{ activeRoute == 'district' ? 'active' }}"
                                                           href="{{ url('district') }}">
                                                        <span class="menu-bullet">
                                                            <span class="bullet bullet-dot"></span>
                                                        </span>
                                                            <span class="menu-title">Districts</span>
                                                        </a>
                                                    </div>
                                                {% endif %}
                                                {% if menu.menuCounty %}
