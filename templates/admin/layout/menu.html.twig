                    {% if menu.menuUnit or menu.menuEquation or menu.menuItem or menu.menuInspector or menu.menuHoliday or
                        menu.menuEmployee or menu.menuMaterial or menu.menuOverhead or menu.menuAdvertisement or menu.menuReminder or
                        menu.menuRol or menu.menuUsuario or menu.menuLog or menu.menuNotification %}

