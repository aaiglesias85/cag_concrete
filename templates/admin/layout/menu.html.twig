                                <!--begin:Menu sub-->
                                <div class="menu-sub menu-sub-accordion">
                                    <!--begin:Menu item-->
                                    {% if menu.menuUnit %}
                                        <div class="menu-item">
                                            <a class="menu-link {{ activeRoute == 'unit' ? 'active' }}"
                                               href="{{ url('unit') }}">
